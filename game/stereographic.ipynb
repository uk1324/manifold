   ],
   "source": [
    "x, y, z = symbols('x y z')\n",
    "s = x * x + y * y + z * z\n",
    "a = 2 / (s + 1)\n",
    "r = Matrix([\n",
    "    x * a,\n",
    "    y * a,\n",
    "    z * a,\n",
    "    (s - 1) / (s + 1)\n",
    "])\n",
    "j = r.jacobian(Matrix([x, y, z]))\n",
    "j = j.subs([(x, 0), (y, 0), (z, 0)])\n",
    "j = simplify(j)\n",
    "display(j)"
   ]
  }
